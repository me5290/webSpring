{{>layouts/header}}
    {{name}}
    <table>
        <thead>
            <tr>
                <th>ID</th>
                <th>Title</th>
                <th>Content</th>
            </tr>
        </thead>
        <tbody>
            {{#article}}
            <tr>
                <td>{{id}}</td>
                <td>{{title}}</td>
                <td>{{content}}</td>
            </tr>
            {{/article}}
        </tbody>
    </table>
    <a href="/article/{{article.id}}/edit">수정</a>
    <a href="/article/{{article.id}}/delete">삭제</a>
    <a href="/article">글 목록으로 가기</a>
{{>layouts/footer}}